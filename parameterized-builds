properties([parameters([choice(choices: ['master', 'feature-1', 'feature-2'], description: 'Choose the branch to build', name: 'branch')]), 

pipelineTriggers([pollSCM('')])])

node{
  stage('Scm Checkout'){
     echo "Pulling pudates from ${params.branch}"
     git url: 'https://github.com/javahometech/my-app', branch: "${params.branch}"
  }
}
